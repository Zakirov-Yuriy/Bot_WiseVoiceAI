{
  "dashboard": {
    "id": null,
    "title": "WiseVoiceAI Bot Monitoring",
    "tags": ["wisevoiceai", "telegram", "bot"],
    "timezone": "UTC",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(bot_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}} {{status}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Transcription Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(bot_transcription_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "rate(bot_transcriptions_total[5m])",
            "legendFormat": "Transcriptions per second"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Cache Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(bot_cache_hits_total[5m]) / (rate(bot_cache_hits_total[5m]) + rate(bot_cache_misses_total[5m])) * 100",
            "legendFormat": "Cache hit rate %"
          },
          {
            "expr": "bot_cache_entries",
            "legendFormat": "{{cache_type}} entries"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Rate Limiting",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(bot_rate_limits_exceeded_total[5m])",
            "legendFormat": "{{limit_type}} violations"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(bot_errors_total[5m])",
            "legendFormat": "{{error_type}} in {{component}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "Active Users",
        "type": "singlestat",
        "targets": [
          {
            "expr": "bot_active_users",
            "legendFormat": "Active users"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "refresh": "5s",
    "schemaVersion": 16,
    "version": 0,
    "links": []
  }
}
